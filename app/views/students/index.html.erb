<h1>学员过程追踪管理-学生列表</h1>
<h3><%= link_to '按日浏览学生成绩', view_students_path %></h3>

<ul>
<% @users.each do |user| %>
	<li>
		<%= link_to user.name, [:admin, user] %>
		<a href="/students/<%= user.id %>/results">成绩管理</a>
		<a href="/students/<%= user.id %>/student_timelines">时间管理</a>
		
	</li>
<% end %>
</ul>

<%=h will_paginate @users, :previous_label => '上一页',  :next_label => '下一页'%>