<h1>Listing web_pages</h1>

<table>
  <tr>
    <th>Domain</th>
    <th>Categroy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @web_pages.each do |web_page| %>
  <tr>
    <td><%= web_page.domain %></td>
    <td><%= web_page.categroy %></td>
    <td><%= link_to 'Show', web_page %></td>
    <td><%= link_to 'Edit', edit_web_page_path(web_page) %></td>
    <td><%= link_to 'Destroy', web_page, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

